            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "palette-classic"
                    },
                    "custom": {
                        "drawStyle": "line",
                        "lineInterpolation": "linear",
                        "lineWidth": 1,
                        "pointSize": 3,
                        "showPoints": "always",
                        "spanNulls": false
                    },
                    "mappings": []
                },
